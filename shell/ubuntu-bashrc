if [ -f ~/.dotfiles/shell/bashrc ]; then
  echo "Loading bashrc"
  . ~/.dotfiles/shell/bashrc
  echo "Loaded ubuntu-bash-rc"
fi

eval $(/home/linuxbrew/.linuxbrew/bin/brew shellenv)

alias pbcopy="xclip -selection c" 
alias pbpaste="xclip -selection clipboard -o" 
